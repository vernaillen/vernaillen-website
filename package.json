{
  "name": "vernaillen-website",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "Wouter Vernaillen",
  "cordovaId": "com.vernaillen.website.app",
  "capacitorId": "",
  "author": "Wouter Vernaillen <wouter@vernaillen.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "test": "echo \"No test specified\" && exit 0",
    "build": "quasar build -m ssr ",
    "docker-build": "npm run build && docker build . -t vernaillen/vernaillen-website:0.0.1",
    "docker-start": "docker run --name vernaillen-website -d -p 3000:3000 vernaillen/vernaillen-website:0.0.1",
    "docker-push": "docker push vernaillen/vernaillen-website:0.0.1",
    "k8s-apply": "kubectl apply -f k8s.yaml ",
    "deploy": "npm run docker-build && npm run docker-push && npm run k8s-apply",
    "dev": "quasar dev -m ssr"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.18.1",
    "quasar": "^1.0.0",
    "vue-i18n": "^8.0.0",
    "vue-tour": "^1.2.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
