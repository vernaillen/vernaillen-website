{
  "name": "vernaillen-website",
  "version": "0.0.2",
  "description": "A Quasar Framework app",
  "productName": "Wouter Vernaillen",
  "cordovaId": "com.vernaillen.website.app",
  "capacitorId": "",
  "author": "Wouter Vernaillen <wouter@vernaillen.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "test": "echo \"No test specified\" && exit 0",
    "build": "quasar build -m pwa ",
    "docker-build": "yarn build && docker build . -t vernaillen/vernaillen-website:0.0.1",
    "docker-start": "docker run --name vernaillen-website -d -p 80:80 vernaillen/vernaillen-website:0.0.1",
    "docker-push": "docker push vernaillen/vernaillen-website:0.0.1",
    "k8s-apply": "kubectl apply -f k8s.yaml ",
    "deploy": "yarn docker-build && yarn docker-push && yarn k8s-apply",
    "dev": "quasar dev -m pwa"
  },
  "dependencies": {
    "@quasar/extras": "^1.5.1",
    "axios": "^0.19.2",
    "quasar": "^1.9.2",
    "vue-i18n": "^8.0.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@quasar/app": "^1.5.6",
    "@quasar/quasar-app-extension-qmarkdown": "^1.0.23",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^6.2.0"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
